{
    "version": 2,
    "builds": [
      {
        "src": "dist/src/index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/test",
        "dest": "dist/src/index.js"
      },
      {
        "src": "/api/(.*)",
        "dest": "dist/src/index.js"
      }
    ]
  }